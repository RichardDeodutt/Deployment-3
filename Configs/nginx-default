server {
        listen 5000;

        root /var/www/html;

        index index.html index.htm index.nginx-debian.html;

        server_name _;

        location / {
                proxy_pass http://127.0.0.1:5000;
                proxy_set_header Host $host;
                proxy_set_header x-Forward-For $proxy_add_x_forwarded_for;
        }
}